{
    "configurations": [
    {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/p02_simulator_RAM/p02_simulator_RAM",
        "args": ["${workspaceFolder}/p02_simulator_RAM/test/test7.ram", "${workspaceFolder}/p02_simulator_RAM/test/test7_input1.txt", "${workspaceFolder}/p02_simulator_RAM/test/output71.txt"],
        "stopAtEntry": true,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }
    ]
}